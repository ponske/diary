{
  "expo": {
    "name": "WonderPasNavi",
    "slug": "wonderpasnavi",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#6B46C1",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*",
      "!assets/data/*.json"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wonderpasnavi.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#6B46C1"
      },
      "package": "com.wonderpasnavi.app"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/"
          ]
        }
      ]
    ]
  }
}
